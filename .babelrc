{
    "presets": [
        [
            "env",
            {
                "modules": false
            }
        ],
        "stage-0",
        "react"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-runtime"
    ],
    "env": {
        "development": {
            "plugins": ["react-hot-loader/babel"]
        },
        "test": {
            "presets": [["es2015"], "stage-0", "react"],
            "plugins": [
                "transform-decorators-legacy",
                "transform-runtime"
            ]
        }
    }
}
